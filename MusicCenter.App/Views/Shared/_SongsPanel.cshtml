@model List<MusicCenter.Common.ViewModels.Band.SongsPanelViewModel>

<ul class="nav nav-tabs" style="margin-bottom:10px">
    <li role="presentation" class="active"><a href="#">New singles</a></li>
</ul>
@if (Model != null)
{
    <div class="list-group">
        @foreach (var song in Model)
        {
            var songUrl = @song.UrlAddress;
                         
            <a href="@Url.Action("BandSingles", "Band", new { BandName = song.BandName})" class="list-group-item">
                 <div class="col-sm-12" >
                      <iframe width="100%" height="20" scrolling="no" frameborder="no" src="https://w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/tracks/@songUrl&amp;color=ff5500&amp;inverse=false&amp;auto_play=false&amp;show_user=false"></iframe>
                 </div>
            </a>
        }
    </div>
}
else
{
    <h1><small>No songs to show</small></h1>
}

